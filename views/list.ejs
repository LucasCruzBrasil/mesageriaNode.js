<h3>Consultas</h3>
<hr>
<form action="/searchResult" method="get">
   
    <input type="search" 
    name="search" 
    id="search" 
    placeholder="digite o e-mail ou CPF">

    <button>Pesquisar</button>

</form>
<hr>
<% appos.forEach(appo => {%>
   
    <h1>Consultas: <%= appo.description %> </h1>
    <h4>Paciente:<%= appo.name %></h4>
    <h4>Email: <%= appo.email %></h4>
    <h4>CPF : <%= appo.cpf %></h4>
    <hr>

 <%}) %>